{
  "manifest_version": 2,
  "name": "ES Scripter",
  "description": "ES Scripter",
  "version": "0.1",
  "permissions": [
    "https://*/", "http://*/",
    "tabs",
    "clipboardWrite"
  ],
  "background": {
    "scripts": [ "background.js" ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'; unsafe-inline 'self'",
  "sandbox": {

  }
}